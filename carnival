from pyo import *

SNDS_PATH = r'C:\reaper\cluster_5_sounds\carnival_3.wav'

s= Server(duplex=1, buffersize=1024, winhost='asio',nchnls=2)
s.boot()
s.start()

sound_player = SfPlayer(SNDS_PATH, loop=False)
ele = 45
azi = Sine(0.1).range(0,360)

binaural_renderer = Binaural(sound_player, azimuth=azi, elevation=ele, azispan=0, elespan=0)
data=binaural_renderer.out()

rec1=Record(data, filename='wheel1.wav', chnls=2, fileformat=0, sampletype=0)

s.gui(locals())
